{
	"name": "dataflow_titlemaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_titlemaster_csv",
						"type": "DatasetReference"
					},
					"name": "source1titlemastercsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_titlemaster_sql_table",
						"type": "DatasetReference"
					},
					"name": "sink1titlemaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as integer,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1titlemastercsv",
				"source1titlemastercsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TitleCode,",
				"          TitleDescription",
				"     )) ~> sink1titlemaster"
			]
		}
	}
}